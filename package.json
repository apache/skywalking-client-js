{
  "name": "skywalking-client-js",
  "version": "0.8.0",
  "description": "Client-side JavaScript exception and tracing library for Apache SkyWalking APM",
  "main": "index.js",
  "types": "lib/src/index.d.ts",
  "repository": "apache/skywalking-client-js",
  "homepage": "skywalking.apache.org",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/apache/skywalking/issues",
    "email": "dev@skywalking.apache.org"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "minimist": "^1.2.3",
    "prettier": "^2.1.1",
    "ts-loader": "^9.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-concat-files-plugin": "^0.5.2",
    "webpack-dev-server": "^3.11.3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "rebuild": "rm -rf ./node_modules && rm -rf ./lib && npm install && npm run build",
    "start": "npx webpack serve",
    "release": "make release-src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "LICENSE",
    "NOTICE",
    "CHANGES.md",
    "README.md",
    "index.js",
    "lib/",
    "dist/licenses"
  ],
  "keywords": [
    "skywalking",
    "observability",
    "apm",
    "distributed-tracing",
    "dapper",
    "javascript",
    "web-performance"
  ],
  "dependencies": {
    "js-base64": "^3.6.0"
  }
}
